    {
      "class_name": "ticket_search",
      "class_description": "search object for tickets",
      "base_class_name": "sys_object",
      "fields": {
        "ticket_name": "string",
        "ticket_description": "string",
        "ticket_class": "string",
        "ticket_object_class": "string",
        "tickets": {
          "field_type": "query",
          "query": {
            "from": [
              {
                "class_name": "work_ticket",
                "name": "work_ticket"
              }
            ],
            "stages": [
              {
                "class_name": "filter",
                "stage_input_name": "work_ticket",
                "condition": {
                  "class_name": "any",
                  "conditions": [
                    {
                      "class_name": "contains",
                      "valuepath": "name",
                      "value": "$this.name"
                    },
                    {
                      "class_name": "contains",
                      "valuepath": "description",
                      "value": "$this.description"
                    },
                    {
                      "class_name": "contains",
                      "valuepath": "status",
                      "value": "$this.status"
                    }
                  ]
                }
              }
            ]
          }
        }
      }
    },
