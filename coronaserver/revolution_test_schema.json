            ],
            "stages": [
              {
                "class_name": "filter",
                "stage_input_name": "committee",
                "condition": {
                  "class_name": "all",
                  "conditions": [
                    {
                      "class_name": "contains",
                      "valuepath": "committee_name",
                      "value": "$this.committee_name"
                    },
                    {
                      "class_name": "contains",
                      "valuepath": "political_party",
                      "value": "$this.political_party"
                    }
                  ]
                }
              }
            ]
          }
        }
