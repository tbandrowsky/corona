{
  "class_name": "page",
  "page_name": "page_object_edit_template",
  "children": [
    {
      "class_name": "column",
      "name": "current_object",
      "box": {
        "width": "1.0 container",
        "height": "1.0 remaining"
      },
      "children": [
        {
          "class_name": "row",
          "name": "current_object",
          "box": {
            "width": "1.0 container",
            "height": "50 pixels"
          },
          "view_style": {
            "text_style": {
              "font_name": "Century Gothic,Arial",
              "font_size": 18,
              "bold": false,
              "underline": false,
              "vertical_alignment": "far"
            },
            "shape_border_thickness": 0,
            "box_border_thickness": 0,
            "shape_fill_brush": {
              "class_name": "solid_brush",
              "color": "FFFFEE"
            },
            "box_fill_brush": {
              "class_name": "solid_brush",
              "color": "000000"
            }
          },
          "children": [
            {
              "class_name": "image",
              "image_filename": "$icon_image_file",
              "box": {
                "width": "50 pixels",
                "height": "50 pixels"
              }
            },
            {
              "class_name": "chaptertitle",
              "text": "{class_name}",
              "box": "chaptertitle_box"
            }
          ]
        },
        {
          "class_name": "row",
          "name": "current_object",
          "box": {
            "width": "1.0 container",
            "height": "1.0 remaining"
          },
          "children": [
            {
              "class_name": "column",
              "name": "object_details",
              "box": {
                "width": ".66 container",
                "height": "1.0 container"
              },
              "children": [
                {
                  "class_name": "paragraph",
                  "text": "",
                  "box": "spacer_row_box"
                },
                "$details_contents"
              ]
            },
            {
              "class_name": "column",
              "name": "notes_column",
              "box": {
                "width": ".33 container",
                "height": "1.0 container"
              },
              "children": [
                {
                  "class_name": "items_view",
                  "name": "notes_table",
                  "json_field_name": "notes",
                  "box": {
                    "width": "1.0 container",
                    "height": "1.0 remaining"
                  },
                  "sources": {
                    "patriarch_note": "card_note"
                  },
                  "empty_text": "No notes"
                }
              ]
            }
          ]
        },
        {
          "class_name": "row",
          "name": "system_details",
          "box": {
            "width": "1.0 container",
            "height": "250 pixels"
          },
          "children": [
            {
              "class_name": "frame",
              "name": "contact_details_container",
              "box": {
                "width": "1.0 container",
                "height": "1.0 container"
              },
              "on_load": {
                "class_name": "select_frame",
                "source_frame": "page_object_details",
                "message": "load frame."
              }
            }
          ]
        }
      ]
    }
  ]
}
