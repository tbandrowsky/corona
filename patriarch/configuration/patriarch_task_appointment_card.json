{
  "class_name": "page",
  "page_name": "card_task_appointment",
  "using": {
    "page_name": "page_object_card_template",
    "parameters": {
      "$icon_image_file": "assets\\appointment.png",
      "$card_height": "75 pixels",
      "$details_contents": [
        {
          "class_name": "paragraph",
          "json_field_name": "status",
          "text": "{status}",
          "box": "card_text_box"
        },
        {
          "class_name": "paragraph",
          "json_field_name": "with",
          "text": "{with} {when} {where}",
          "box": "card_text_box"
        }
      ]
    }
  }
}
