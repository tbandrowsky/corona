{
  "class_name": "page",
  "page_name": "card_table",
  "using": {
    "page_name": "page_object_card_template",
    "parameters": {
      "$icon_image_file": "assets\\database.png",
      "$card_height": "75 pixels",
      "$details_contents": [
        {
          "class_name": "paragraph",
          "name": "description",
          "box": "card_text_box",
          "json_field_name": "name",
          "text": "{description}"
        },
        {
          "class_name": "paragraph",
          "name": "counts",
          "box": "card_text_box",
          "json_field_name": "object_count",
          "text": "{object_count} objects, {word_count} words"
        }
      ]
    }
  }
}
