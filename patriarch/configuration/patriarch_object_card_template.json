{
  "class_name": "page",
  "page_name": "page_object_card_template",
  "children": [
    {
      "class_name": "row",
      "name": "current_object",
      "box": {
        "width": "1.0 container",
        "height": "$card_height"
      },
      "view_style": {
        "text_style": {
          "font_name": "Century Gothic,Arial",
          "font_size": 14,
          "bold": false,
          "underline": false,
          "vertical_alignment": "near"
        },
        "shape_border_thickness": 0,
        "box_border_thickness": 0,
        "shape_fill_brush": {
          "class_name": "solid_brush",
          "color": "FFFFEE"
        },
        "box_fill_brush": {
          "class_name": "solid_brush",
          "color": "202020"
        }
      },
      "children": [
        {
          "class_name": "image",
          "image_filename": "$icon_image_file",
          "box": {
            "width": "50 pixels",
            "height": "50 pixels"
          }
        },
        {
          "class_name": "column",
          "name": "current_object",
          "box": {
            "width": "1.0 remaining",
            "height": "1.0 container"
          },
          "children": [
            {
              "class_name": "paragraph",
              "text": "",
              "box": "card_text_box"
            },
            {
              "class_name": "chaptersubtitle",
              "text": "{class_name}",
              "box": "chaptersubtitle_box"
            },
            "$details_contents"
          ]
        }
      ]
    }
  ]
}