{
  "class_name": "page",
  "page_name": "card_task_trip",
  "using": {
    "page_name": "page_object_card_template",
    "parameters": {
      "$icon_image_file": "assets\\trip.png",
      "$card_height": "75 pixels",
      "$details_contents": [
        {
          "class_name": "paragraph",
          "json_field_name": "status",
          "text": "{status}"
        },
        {
          "class_name": "paragraph",
          "json_field_name": "with",
          "text": "{with} {when} {where}"
        }
      ]
    }
  }
}
