{
  "class_name": "page",
  "page_name": "card_property_gun",
  "using": {
    "page_name": "page_object_card_template",
    "parameters": {
      "$icon_image_file": "assets\\gun.png",
      "$card_height": "75 pixels",
      "$details_contents": [
        {
          "class_name": "paragraph",
          "json_field_name": "description",
          "text": "{description}",
          "box": "card_text_box"
        },
        {
          "class_name": "paragraph",
          "json_field_name": "model_number",
          "text": "{model_number}/{serial_number}",
          "box": "card_text_box"
        }
      ]
    }
  }
}
