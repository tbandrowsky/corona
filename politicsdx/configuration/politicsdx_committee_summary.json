{
  "class_name": "page",
  "page_name": "page_committee_summary",
  "children": [
    {
      "class_name": "row",
      "name": "committee_summary_object",
      "box": "full_container",
      "children": [
        {
          "class_name": "column",
          "name": "committee_details",
          "box": {
            "width": "400 pixels",
            "height": "1.0 container"
          },
          "children": [
            {
              "class_name": "chaptertitle",
              "text": "Committee",
              "box": "chaptertitle_box"
            },
            {
              "class_name": "paragraph",
              "text": "Committee Name:",
              "box": "edit_field_box"
            },
            {
              "class_name": "edit",
              "json_field_name": "committee_name",
              "box": "edit_field_large_box"
            },
            {
              "class_name": "paragraph",
              "text": "Committee ID:",
              "box": "edit_label_box"
            },
            {
              "class_name": "edit",
              "json_field_name": "committee_id",
              "box": "edit_field_box"
            },
            {
              "class_name": "paragraph",
              "text": "Committee Type:",
              "box": "edit_label_box"
            },
            {
              "class_name": "paragraph",
              "json_field_name": "committee_type",
              "box": "edit_field_box",
              "text": "{committee_type}, {committee_designation}, {committee_filing_frequency}"
            },
            {
              "class_name": "paragraph",
              "text": "Committee Receipts:",
              "box": "edit_label_box"
            },
            {
              "class_name": "paragraph",
              "json_field_name": "committee_receipts",
              "box": "edit_field_large_box",
